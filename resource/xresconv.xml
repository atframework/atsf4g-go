<?xml version="1.0" encoding="UTF-8"?>
<!-- <?xml-stylesheet type="text/xsl" href="helper/view.xsl"?> -->
<root>
<!--
# @see 转表工具文档: https://xresloader.atframe.work/
# @see 验证器文档: https://github.com/owent/xresloader?tab=readme-ov-file#%E9%AA%8C%E8%AF%81%E5%99%A8
-->
  <global>
    <java_option desc="java选项-最大内存限制2GB">-Xmx2048m</java_option>
    <java_option desc="java选项-客户端模式">-client</java_option>
    <java_option desc="java选项-默认时区">-Duser.timezone=Asia/Shanghai</java_option>

    <default_scheme name="KeyRow" desc="默认scheme模式参数-Key行号">2</default_scheme>
    <!--<default_scheme name="UeCg-CsvObjectWrapper" desc="Ue-Csv输出的包裹字符">{|}</default_scheme>-->

    <option desc="自定义验证器">--validator-rules validator.yaml</option>
  </global>

  <category desc="类信息">
    <!--
      <tree id="all_cats" name="大分类">
          <tree id="kind" name="角色配置"></tree>
      </tree>
      -->
    <tree id="misc" name="杂项"></tree>
    <tree id="item" name="道具"></tree>
    <tree id="user" name="玩家/用户"></tree>
    <tree id="quest" name="任务"></tree>
    <tree id="unlock" name="模块解锁"></tree>
    <tree id="mall" name="商城"></tree>
  </category>

  <list>
    <item name="常量表" cat="misc" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Const.xlsx|常量|2,2,0,2</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelConstConfig</scheme>
      <scheme name="OutputFile" desc="输出文件名">const_settings.bytes</scheme>

      <scheme name="KeyRow">1</scheme>
      <option desc="反转行列">--transpose-data-source</option>
    </item>
    <item name="道具描述总表" cat="item" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Item.xlsx|道具描述总表|3,1</scheme>
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Item.xlsx|辅助道具(不可获得)|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelItem</scheme>
      <scheme name="OutputFile" desc="输出文件名">item.bytes</scheme>
    </item>
    <item name="道具周期重置道具" cat="item" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Item.xlsx|周期重置道具|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelItemReset</scheme>
      <scheme name="OutputFile" desc="输出文件名">item_reset.bytes</scheme>
    </item>
    <item name="通用条件表" cat="misc" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Condition.xlsx|限制条件|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelConditionPool</scheme>
      <scheme name="OutputFile" desc="输出文件名">condition_pool.bytes</scheme>
    </item>
    <item name="用户等级表" cat="user" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">User.xlsx|用户等级表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelUserLevel</scheme>
      <scheme name="OutputFile" desc="输出文件名">user_level.bytes</scheme>
    </item>
    <item name="新用户初始道具表" cat="user" class="server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">User.xlsx|新用户初始道具表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelUserInitializeItems</scheme>
      <scheme name="OutputFile" desc="输出文件名">user_initialize_items.bytes</scheme>
    </item>
    <item name="随机池表" cat="misc" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">RandomPool.xlsx|RandomPool|3,1</scheme>
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">RandomPool.xlsx|RandomPoolMerge|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelRandomPool</scheme>
      <scheme name="OutputFile" desc="输出文件名">random_pool.bytes</scheme>
    </item>
    <item name="任务列表" cat="quest" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Quest.xlsx|Quest|4,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelQuestList</scheme>
      <scheme name="OutputFile" desc="输出文件名">quest_list.bytes</scheme>
      <scheme name="KeyRow" desc="scheme模式参数-Key行号">3</scheme>
    </item>
    <item name="任务进度类型" cat="quest" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Quest.xlsx|QuestProgressType|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelQuestProgressType</scheme>
      <scheme name="OutputFile" desc="输出文件名">quest_progress_type.bytes</scheme>
    </item>
    <item name="任务触发事件类型" cat="quest" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Quest.xlsx|QuestTriggerEventType|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelQuestTriggerEventType</scheme>
      <scheme name="OutputFile" desc="输出文件名">quest_trigger_event_type.bytes</scheme>
    </item>
    <item name="模块解锁表" cat="unlock" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">ModuleUnlock.xlsx|模块解锁表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelModuleUnlockType</scheme>
      <scheme name="OutputFile" desc="输出文件名">module_unlock_type.bytes</scheme>
    </item>
    <item name="激活上报解锁表" cat="misc" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Activate.xlsx|激活上报解锁表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelActivate</scheme>
      <scheme name="OutputFile" desc="输出文件名">user_activate.bytes</scheme>
    </item>
    <item name="商城表" cat="mall" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Mall.xlsx|商城表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelMall</scheme>
      <scheme name="OutputFile" desc="输出文件名">mall.bytes</scheme>
    </item>
    <item name="商城页签表" cat="mall" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Mall.xlsx|商城页签表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelMallSheet</scheme>
      <scheme name="OutputFile" desc="输出文件名">mall_sheet.bytes</scheme>
    </item>
    <item name="商城商品表" cat="mall" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Mall.xlsx|商城商品表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelMallProduct</scheme>
      <scheme name="OutputFile" desc="输出文件名">mall_product.bytes</scheme>
    </item>
  </list>
</root>

<?xml version="1.0" encoding="UTF-8"?>
<!-- <?xml-stylesheet type="text/xsl" href="helper/view.xsl"?> -->
<root>
  <global>
    <java_option desc="java选项-最大内存限制2GB">-Xmx2048m</java_option>
    <java_option desc="java选项-客户端模式">-client</java_option>
    <java_option desc="java选项-默认时区">-Duser.timezone=UTC</java_option>

    <default_scheme name="KeyRow" desc="默认scheme模式参数-Key行号">2</default_scheme>
    <!--<default_scheme name="UeCg-CsvObjectWrapper" desc="Ue-Csv输出的包裹字符">{|}</default_scheme>-->

    <option desc="自定义验证器">--validator-rules validator.yaml</option>
  </global>

  <category desc="类信息">
    <!--
      <tree id="all_cats" name="大分类">
          <tree id="kind" name="角色配置"></tree>
      </tree>
      -->
    <tree id="misc" name="杂项"></tree>
    <tree id="item" name="道具"></tree>
    <tree id="user" name="玩家/用户"></tree>
  </category>

  <list>
    <item name="常量表" cat="misc" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Const.xlsx|常量|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelOriginConstConfig</scheme>
      <scheme name="OutputFile" desc="输出文件名">const.bytes</scheme>
    </item>
    <item name="道具描述总表" cat="item" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Item.xlsx|道具描述总表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelItem</scheme>
      <scheme name="OutputFile" desc="输出文件名">item.bytes</scheme>
    </item>
    <item name="角色表" cat="" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Role.xlsx|角色表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelRole</scheme>
      <scheme name="OutputFile" desc="输出文件名">role.bytes</scheme>
    </item>
    <item name="升级经验表" cat="" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Role.xlsx|升级经验表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelRoleExpUpgrade</scheme>
      <scheme name="OutputFile" desc="输出文件名">role_exp_upgrade.bytes</scheme>
    </item>
    <item name="技能表" cat="" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Skill.xlsx|技能|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelSkill</scheme>
      <scheme name="OutputFile" desc="输出文件名">skill.bytes</scheme>
    </item>
    <item name="通用条件表" cat="misc" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">Condition.xlsx|限制条件|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelConditionPool</scheme>
      <scheme name="OutputFile" desc="输出文件名">condition_pool.bytes</scheme>
    </item>
    <item name="用户等级表" cat="user" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">User.xlsx|用户等级表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelUserLevel</scheme>
      <scheme name="OutputFile" desc="输出文件名">user_level.bytes</scheme>
    </item>
    <item name="新用户初始道具表" cat="user" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">User.xlsx|新用户初始道具表|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelUserInitializeItems</scheme>
      <scheme name="OutputFile" desc="输出文件名">user_initialize_items.bytes</scheme>
    </item>
    <item name="随机池表" cat="misc" class="client server">
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">RandomPool.xlsx|RandomPool|3,1</scheme>
      <scheme name="DataSource" desc="数据源(文件名|表名|数据起始行号,数据起始列号)">RandomPool.xlsx|RandomPoolMerge|3,1</scheme>
      <scheme name="ProtoName" desc="协议名">proy.config.ExcelRandomPool</scheme>
      <scheme name="OutputFile" desc="输出文件名">random_pool.bytes</scheme>
    </item>
  </list>
</root>
